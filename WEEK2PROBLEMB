#include<bits/stdc++.h>
using namespace std;
int main()
{
    int t;
    cin>>t;
    while(t--)
    {
       int n,k,a,cnt=0;
       cin>>n>>k;
       for(int i=1;i<=n;i++)
       {
           cin>>a;
           if(a<=0)
            cnt++;
       }
       if(cnt>=k)
       {
           cout<<"NO"<<endl;
       }
       else
       {
           cout<<"YES"<<endl;
       }
    }
}
